#Lammps input file generated by ForcePy

units 		real
timestep    1.0
atom_style  full
read_data   data.lammps

pair_style  table linear 10000

pair_coeff  1 1 cg_force_pair.table SF_Pairwise_CIS_CIS 15
pair_coeff  1 2 cg_force_pair.table SF_Pairwise_CIS_TRANS 15
pair_coeff  2 2 cg_force_pair.table SF_Pairwise_TRANS_TRANS 15

velocity    all create 300.0 2374323

fix	    1 all nvt temp 300.0 300.0 100.0
#fix	    2 all mdi
fix         UCG all ucg_gfrd 2222 Beta 1.6775 nspecies 1 nstates 2 speciesbeads 1 aoffset 1 boffset 0 rates rates.in mhcorr mhcorr.in restrictmol restrictmol.in
thermo      1
thermo_style custom step temp ke pe etotal
run         10
#mdi
